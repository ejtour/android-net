<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content"
    android:layout_width="match_parent"
    android:layout_height="30dp"
    android:background="@android:color/white">

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/color_dddddd"/>

    <TextView
        android:id="@+id/txt_saleman_code"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:singleLine="true"
        android:textAppearance="@style/TextAppearance.City22.Little"
        tools:text="编码"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/color_dddddd"/>

    <TextView
        android:id="@+id/txt_saleman_name"
        android:layout_width="80dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:singleLine="true"
        android:textAppearance="@style/TextAppearance.City22.Little"
        tools:text="姓名"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/color_dddddd"/>

    <TextView
        android:id="@+id/txt_intent_customer"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:singleLine="true"
        android:textAppearance="@style/TextAppearance.City22.Little"
        tools:text="意向客户数"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/color_dddddd"/>

    <TextView
        android:id="@+id/txt_sign_customer"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:singleLine="true"
        android:textAppearance="@style/TextAppearance.City22.Little"
        tools:text="签约客户数"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/color_dddddd"/>

    <TextView
        android:id="@+id/txt_sign_shop"
        android:layout_width="70dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:singleLine="true"
        android:textAppearance="@style/TextAppearance.City22.Little"
        tools:text="签约门店"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/color_dddddd"/>

    <TextView
        android:id="@+id/txt_incr_intent_customer"
        android:layout_width="70dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:singleLine="true"
        android:textAppearance="@style/TextAppearance.City22.Little"
        tools:text="新增意向客户"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/color_dddddd"/>

    <TextView
        android:id="@+id/txt_incr_sign_customer"
        android:layout_width="70dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:singleLine="true"
        android:textAppearance="@style/TextAppearance.City22.Little"
        tools:text="新增签约客户"/>
    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="@color/color_dddddd"/>

    <TextView
        android:id="@+id/txt_incr_sign_shop"
        android:layout_width="70dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:singleLine="true"
        android:textAppearance="@style/TextAppearance.City22.Little"
        tools:text="新增签约门店"/>
</LinearLayout>
