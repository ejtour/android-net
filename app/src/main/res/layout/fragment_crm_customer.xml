<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">

    <TextView
        android:id="@+id/fcc_title_bar"
        android:layout_width="0dp"
        android:layout_height="@dimen/title_bar_height"
        android:background="@drawable/base_bg_shadow_top_bar"
        android:gravity="center"
        android:text="客户管理"
        android:textColor="@android:color/white"
        android:textSize="@dimen/base_text_18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/fcc_intent_btn"
        android:layout_width="0dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_title_bar" />

    <View
        android:id="@+id/fcc_div_1"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="69dp"
        android:background="@color/color_f3f3f3"
        app:layout_constraintBottom_toBottomOf="@id/fcc_intent_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/fcc_intent_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_crm_customer_intent"
        app:layout_constraintBottom_toBottomOf="@id/fcc_div_1"
        app:layout_constraintEnd_toStartOf="@id/fcc_div_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_title_bar" />

    <TextView
        android:id="@+id/fcc_intent_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="意向客户"
        android:textColor="@color/color_333333"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@id/fcc_div_1"
        app:layout_constraintTop_toBottomOf="@id/fcc_title_bar" />

    <TextView
        android:id="@+id/fcc_intent_left"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="12dp"
        android:singleLine="true"
        android:text="我的意项客户0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toTopOf="@id/fcc_div_1"
        app:layout_constraintEnd_toStartOf="@id/fcc_intent_right"
        app:layout_constraintStart_toStartOf="@id/fcc_div_1" />

    <TextView
        android:id="@+id/fcc_intent_right"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:singleLine="true"
        android:text="我的全部客户0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@id/fcc_intent_left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/fcc_intent_left" />

    <View
        android:id="@+id/fcc_partner_btn"
        android:layout_width="0dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_intent_btn" />

    <View
        android:id="@+id/fcc_div_2"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="69dp"
        android:background="@color/color_f3f3f3"
        app:layout_constraintBottom_toBottomOf="@id/fcc_partner_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/fcc_partner_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_crm_customer_partner"
        app:layout_constraintBottom_toBottomOf="@id/fcc_div_2"
        app:layout_constraintEnd_toStartOf="@id/fcc_div_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_div_1" />

    <TextView
        android:id="@+id/fcc_partner_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="合作客户"
        android:textColor="@color/color_333333"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@id/fcc_div_2"
        app:layout_constraintTop_toBottomOf="@id/fcc_div_1" />

    <TextView
        android:id="@+id/fcc_partner_left"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="12dp"
        android:singleLine="true"
        android:text="我的合作门店0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toTopOf="@id/fcc_div_2"
        app:layout_constraintEnd_toStartOf="@id/fcc_partner_right"
        app:layout_constraintStart_toStartOf="@id/fcc_div_2" />

    <TextView
        android:id="@+id/fcc_partner_right"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:singleLine="true"
        android:text="全部合作门店0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@id/fcc_partner_left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/fcc_partner_left" />

    <View
        android:id="@+id/fcc_seas_btn"
        android:layout_width="0dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_partner_btn" />

    <View
        android:id="@+id/fcc_div_3"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="69dp"
        android:background="@color/color_f3f3f3"
        app:layout_constraintBottom_toBottomOf="@id/fcc_seas_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/fcc_seas_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_crm_customer_seas"
        app:layout_constraintBottom_toBottomOf="@id/fcc_div_3"
        app:layout_constraintEnd_toStartOf="@id/fcc_div_3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_div_2" />

    <TextView
        android:id="@+id/fcc_seas_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="客户公海"
        android:textColor="@color/color_333333"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@id/fcc_div_3"
        app:layout_constraintTop_toBottomOf="@id/fcc_div_2" />

    <TextView
        android:id="@+id/fcc_seas_left"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="12dp"
        android:singleLine="true"
        android:text="意向客户0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toTopOf="@id/fcc_div_3"
        app:layout_constraintEnd_toStartOf="@id/fcc_seas_right"
        app:layout_constraintStart_toStartOf="@id/fcc_div_3" />

    <TextView
        android:id="@+id/fcc_seas_right"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:singleLine="true"
        android:text="合作客户0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@id/fcc_seas_left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/fcc_seas_left" />

    <View
        android:id="@+id/fcc_plan_btn"
        android:layout_width="0dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_seas_btn" />

    <View
        android:id="@+id/fcc_div_4"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="69dp"
        android:background="@color/color_f3f3f3"
        app:layout_constraintBottom_toBottomOf="@id/fcc_plan_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/fcc_plan_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_crm_customer_plan"
        app:layout_constraintBottom_toBottomOf="@id/fcc_div_4"
        app:layout_constraintEnd_toStartOf="@id/fcc_div_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_div_3" />

    <TextView
        android:id="@+id/fcc_plan_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="拜访计划"
        android:textColor="@color/color_333333"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@id/fcc_div_4"
        app:layout_constraintTop_toBottomOf="@id/fcc_div_3" />

    <TextView
        android:id="@+id/fcc_plan_left"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="12dp"
        android:singleLine="true"
        android:text="今日拜访计划0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toTopOf="@id/fcc_div_4"
        app:layout_constraintEnd_toStartOf="@id/fcc_plan_right"
        app:layout_constraintStart_toStartOf="@id/fcc_div_4" />

    <TextView
        android:id="@+id/fcc_plan_right"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:singleLine="true"
        android:text="已完成0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@id/fcc_plan_left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/fcc_plan_left" />

    <View
        android:id="@+id/fcc_record_btn"
        android:layout_width="0dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_plan_btn" />

    <View
        android:id="@+id/fcc_div_5"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="69dp"
        android:background="@color/color_f3f3f3"
        app:layout_constraintBottom_toBottomOf="@id/fcc_record_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/fcc_record_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_crm_customer_record"
        app:layout_constraintBottom_toBottomOf="@id/fcc_div_5"
        app:layout_constraintEnd_toStartOf="@id/fcc_div_5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fcc_div_4" />

    <TextView
        android:id="@+id/fcc_record_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="拜访记录"
        android:textColor="@color/color_333333"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@id/fcc_div_5"
        app:layout_constraintTop_toBottomOf="@id/fcc_div_4" />

    <TextView
        android:id="@+id/fcc_record_left"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="12dp"
        android:singleLine="true"
        android:text="今日拜访0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toTopOf="@id/fcc_div_5"
        app:layout_constraintEnd_toStartOf="@id/fcc_record_right"
        app:layout_constraintStart_toStartOf="@id/fcc_div_5" />

    <TextView
        android:id="@+id/fcc_record_right"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:singleLine="true"
        android:text="有效拜访0个"
        android:textColor="@color/color_999999"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@id/fcc_record_left"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/fcc_record_left" />

    <ImageView
        android:id="@+id/fcc_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="19dp"
        android:src="@drawable/ic_crm_customer_add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>