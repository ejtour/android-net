<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.hll_sc_app.widget.TitleBar
        android:id="@+id/wds_header"
        android:layout_width="0dp"
        android:layout_height="@dimen/title_bar_height"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tb_title="收支详情" />

    <View
        android:id="@+id/wds_bg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="5dp"
        android:background="@color/base_white"
        app:layout_constraintBottom_toBottomOf="@+id/wds_content_remark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wds_header" />

    <View
        android:id="@+id/wds_divider"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="@dimen/dp_10"
        android:background="@color/color_eeeeee"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/wds_bg" />

    <TextView
        android:id="@+id/wds_money_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="金额"
        android:textAppearance="@style/TextAppearance.City22.Middle.Color666"
        app:layout_constraintBottom_toTopOf="@+id/wds_divider"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toTopOf="@+id/wds_bg" />

    <TextView
        android:id="@+id/wds_money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color_ed5655"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/wds_money_label"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        app:layout_constraintTop_toTopOf="@+id/wds_money_label"
        tools:text="+997.00" />

    <TextView
        android:id="@+id/wds_title_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="交易类型"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toBottomOf="@+id/wds_divider" />

    <TextView
        android:id="@+id/wds_title_fee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="交易费用"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toBottomOf="@+id/wds_title_type" />

    <TextView
        android:id="@+id/wds_title_balance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="交易余额"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toBottomOf="@+id/wds_title_fee" />

    <TextView
        android:id="@+id/wds_title_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="交易时间"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_title_water"
        app:layout_constraintTop_toBottomOf="@+id/wds_title_balance" />

    <TextView
        android:id="@+id/wds_title_water"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="流水单号"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toBottomOf="@+id/wds_title_time" />

    <TextView
        android:id="@+id/wds_title_pay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="支付单号"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toBottomOf="@+id/wds_title_water" />

    <TextView
        android:id="@+id/wds_title_business"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="业务单号"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toBottomOf="@+id/wds_title_pay" />

    <TextView
        android:id="@+id/wds_title_purchaser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="付款集团"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toBottomOf="@+id/wds_title_business" />

    <TextView
        android:id="@+id/wds_title_shop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="付款门店"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toBottomOf="@+id/wds_title_purchaser" />

    <TextView
        android:id="@+id/wds_title_remark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="备注"
        android:textAppearance="@style/TextAppearance.City22.Small.Color999"
        app:layout_constraintStart_toStartOf="@+id/wds_divider"
        app:layout_constraintTop_toBottomOf="@+id/wds_title_shop" />

    <TextView
        android:id="@+id/wds_content_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintBaseline_toBaselineOf="@+id/wds_title_type"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        tools:text="收入" />

    <TextView
        android:id="@+id/wds_fee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintBaseline_toBaselineOf="@id/wds_title_fee"
        app:layout_constraintEnd_toEndOf="@id/wds_divider"
        tools:text="¥20.00" />

    <TextView
        android:id="@+id/wds_content_balance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintBaseline_toBaselineOf="@+id/wds_title_balance"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        tools:text="¥2990.00" />

    <TextView
        android:id="@+id/wds_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintBaseline_toBaselineOf="@+id/wds_title_time"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        tools:text="2018-08-03 11:22:33" />

    <TextView
        android:id="@+id/wds_content_water"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintBaseline_toBaselineOf="@+id/wds_title_water"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        tools:text="21031280381023912" />

    <TextView
        android:id="@+id/wds_content_pay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintBaseline_toBaselineOf="@+id/wds_title_pay"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        tools:text="0323847434" />

    <TextView
        android:id="@+id/wds_content_business"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintBaseline_toBaselineOf="@+id/wds_title_business"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        tools:text="20183949498333" />

    <TextView
        android:id="@+id/wds_content_purchaser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintBaseline_toBaselineOf="@+id/wds_title_purchaser"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        tools:text="二十二城客户集团" />

    <TextView
        android:id="@+id/wds_content_shop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintBaseline_toBaselineOf="@+id/wds_title_shop"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        tools:text="二十二城门店" />

    <TextView
        android:id="@+id/wds_content_remark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:maxWidth="200dp"
        android:paddingBottom="20dp"
        android:textAppearance="@style/TextAppearance.City22.Small.Color666"
        app:layout_constraintEnd_toEndOf="@+id/wds_divider"
        app:layout_constraintTop_toTopOf="@+id/wds_title_remark" />
</androidx.constraintlayout.widget.ConstraintLayout>