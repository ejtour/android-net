<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/base_bg_white_radius_5_solid"
    android:paddingBottom="20dp"
    tools:layout_margin="@dimen/dp_10">

    <TextView
        android:id="@+id/aeh_license_label"
        style="@style/InfoLabel"
        android:text="营业执照"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.hll_sc_app.base.utils.glide.GlideImageView
        android:id="@+id/aeh_license"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_10"
        app:layout_constraintDimensionRatio="1"
        app:layout_constraintHorizontal_bias="0"
        tools:layout_constraintWidth_percent="0.2"
        android:background="@drawable/bg_order_reject_edit"
        app:base_error="@android:color/transparent"
        app:base_preview="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/aeh_license_label" />
</android.support.constraint.ConstraintLayout>