<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--三角类-->
    <declare-styleable name="TriangleView">
        <attr name="trv_color" format="color" />
        <attr name="trv_direction">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="right" value="2" />
            <enum name="left" value="3" />
        </attr>
    </declare-styleable>

    <!--头部HeaderBar类的属性-->
    <declare-styleable name="TitleBar">
        <attr name="tb_title" format="string" />
        <attr name="tb_rightVisibility" format="boolean" />
        <attr name="tb_rightText" format="string" />
        <attr name="tb_rightImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="ImageUploadGroup">
        <attr name="iug_padding" format="dimension" />
        <attr name="iug_margin" format="dimension" />
        <attr name="iug_label" format="string" />
    </declare-styleable>

    <declare-styleable name="ThumbnailView">
        <attr name="ctv_padding" format="dimension" />
        <attr name="ctv_margin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AfterSalesActionBar">
        <attr name="sab_item" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CouponRuleSelectView">
        <attr name="modal" format="enum">
            <enum name="edit" value="0" />
            <enum name="check" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="AlignTextView">
        <attr name="align" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>
</resources>